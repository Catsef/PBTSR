{
  "name": "PBTSR",
  "tree": {
    "$className": "DataModel",

    "ServerScriptService": {
      "$path": "src/server",
      "ServerPackages": {
        "$path": "ServerPackages",
        "_Index": {
          "$path": "ServerPackages/_Index"
        }
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}